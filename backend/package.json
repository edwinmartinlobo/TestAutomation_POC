{
  "name": "test-automation-backend",
  "version": "1.0.0",
  "description": "Backend API for AI-powered test automation framework",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "migrate": "tsx src/database/migrate.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@prisma/client": "^5.9.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4",
    "axios": "^1.6.5",
    "webdriverio": "^8.31.0",
    "@wdio/cli": "^8.31.0",
    "@wdio/local-runner": "^8.31.0",
    "@wdio/mocha-framework": "^8.31.0",
    "@wdio/spec-reporter": "^8.31.0",
    "appium": "^2.4.1",
    "appium-uiautomator2-driver": "^2.41.3",
    "appium-xcuitest-driver": "^5.12.2",
    "sharp": "^0.33.2",
    "minio": "^7.1.3",
    "pg": "^8.11.3",
    "bullmq": "^5.1.5",
    "ioredis": "^5.3.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.12.0",
    "uuid": "^9.0.1",
    "date-fns": "^3.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.11.5",
    "@types/uuid": "^9.0.7",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "prisma": "^5.9.1",
    "prettier": "^3.2.4"
  }
}
